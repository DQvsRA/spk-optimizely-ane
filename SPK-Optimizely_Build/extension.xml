<extension xmlns="http://ns.adobe.com/air/extension/18.0">
	<id>com.speakaboos.ane.OptimizelyANE</id>
	<versionNumber>1.0.0</versionNumber>
	<platforms>
	    
	    <platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>SPK-Optimizely.jar</nativeLibrary>
				<initializer>com.speakaboos.ane.optimizely.SpeakaboosANE</initializer>
				<finalizer>com.speakaboos.ane.optimizely.SpeakaboosANE</finalizer>
			</applicationDeployment>
		</platform>
	    
		<platform name="iPhone-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>libOptimizelyANE.a</nativeLibrary> 
                <initializer>OptimizelyANEInitializer</initializer>
				<finalizer>OptimizelyANEFinalizer</finalizer>
            </applicationDeployment> 
        </platform>
        
		<platform name="iPhone-x86"> 
            <applicationDeployment> 
                <nativeLibrary>libOptimizelyANE.a</nativeLibrary> 
                <initializer>OptimizelyANEInitializer</initializer>
				<finalizer>OptimizelyANEFinalizer</finalizer>
            </applicationDeployment> 
        </platform>
        
		<platform name="default">
			<applicationDeployment />
		</platform>


	</platforms>
</extension>
